generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model policy_account {
  Account     String   @unique @db.VarChar(225)
  Description String   @db.Text
  AccountCode String   @db.VarChar(225)
  COM         Boolean
  TPL         Boolean
  MAR         Boolean
  FIRE        Boolean
  G02         Boolean
  G13         Boolean
  G16         Boolean
  MSPR        Boolean
  PA          Boolean
  CGL         Boolean
  Inactive    Boolean
  createdAt   DateTime @default(now())
  update      DateTime @default(now())
}

model bank {
  Bank_Code String  @id @db.VarChar(10)
  Bank      String  @db.VarChar(50)
  Inactive  Boolean
}

model mortgagee {
  Policy    String   @db.VarChar(100)
  Mortgagee String   @unique @db.VarChar(255)
  createdAt DateTime @default(now())
  update    DateTime @default(now())
}

model subline {
  ID          String   @id @default(uuid())
  Line        String   @db.Text
  SublineName String   @db.VarChar(255)
  createdAt   DateTime @default(now())
  update      DateTime @default(now())
}

model subline_line {
  LineId String @id @default(uuid())
  Line   String @db.VarChar(255)
}

model rates {
  ID        String   @id @default(uuid())
  Account   String?  @db.VarChar(250)
  Line      String?  @db.VarChar(250)
  Type      String?  @db.VarChar(250)
  Rate      String?  @db.VarChar(250)
  createdAt DateTime @default(now())
  update    DateTime @default(now())
}

model ctplregistration {
  ctplId        String   @id
  Prefix        String?  @db.VarChar(50)
  NumSeriesFrom String   @db.VarChar(450)
  NumSeriesTo   String   @db.VarChar(450)

  CreatedBy     String?  @db.VarChar(50)
  createdAt     DateTime @default(now())
  update        DateTime @default(now())
  ctplType      String?  @db.VarChar(250)
}

model ctplprefix {
  prefixId   Int      @id @default(autoincrement())
  prefixName String   @db.VarChar(50)
  createdAt  DateTime @default(now())
  update     DateTime @default(now())
}

model ctpltype {
  typeId    Int      @id @default(autoincrement())
